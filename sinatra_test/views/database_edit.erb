<h2>Post</h2>

<form method="PATCH" action="/db">
  <p>Title: <input type="text" name="title" value="<%= @database[:title] %>"></p>
  <p>Content: <input type="text" name="content" value="<%= @database[:content] %>" ></p>
  <p>Photo: <input type="text" name="photo" value="<%= @database[:photo] %>" ></p>
  <p>Rating: <input type="number" value="<%= @database[:rating] %>" ></p>
  <input type="submit">
</form>
